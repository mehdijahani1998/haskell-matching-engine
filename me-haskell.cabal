-- Initial me-haskell.cabal generated by cabal init.  For further
-- documentation, see http://haskell.org/cabal/users-guide/

-- The name of the package.
name:                me-haskell

-- The package version.  See the Haskell package versioning policy (PVP)
-- for standards guiding when and how versions should be incremented.
-- https://wiki.haskell.org/Package_versioning_policy
-- PVP summary:      +-+------- breaking API changes
--                   | | +----- non-breaking API additions
--                   | | | +--- code changes with no API change
version:             0.1.0.0

-- A short (one-line) description of the package.
-- synopsis:

-- A longer description of the package.
-- description:

-- The license under which the package is released.
license:             GPL-3

-- The file containing the license text.
license-file:        LICENSE

-- The package author(s).
author:              Ramtin Khosravi, Hadi Safari

-- An email address to which users can send suggestions, bug reports, and
-- patches.
maintainer:          Ramtin Khosravi <ramtung@gmail.com>, Hadi Safari <hadi@hadisafari.ir>

-- A copyright notice.
copyright:           (c) 2020-2021 Ramtin Khosravi, Hadi Safari

-- category:

stability:           alpha

build-type:          Simple

-- Extra files to be distributed with the package, such as examples or a
-- README.
extra-source-files:  CHANGELOG.md

-- Constraint on the version of Cabal needed to build this package.
cabal-version:       >=1.10


executable RandomTest
  -- .hs or .lhs file containing the Main module.
  main-is:             RandomTest.hs

  -- Modules included in this executable, other than Main.
  other-modules:
    Decorators.CreditLimit
    Decorators.FillAndKill
    Decorators.MinQuantity
    Decorators.OrderHandler
    Decorators.Ownership
    Decorators.PriceBand
    Decorators.Validation
    Domain.ME
    Domain.MEService
    Domain.Printer
    Infra.Coverage
    Infra.Decorator
    Infra.Shahlaa

  hs-source-dirs:      src

  -- LANGUAGE extensions used by modules in this package.
  other-extensions:    ExplicitForAll

  -- Other library packages from which modules are imported.
  build-depends:       base >=4.12 && <4.16, containers >=0.6 && <0.7, transformers >=0.5 && <0.6, random >=1.2 && <1.3

  -- Directories containing source files.
  -- hs-source-dirs:

  -- Base language which the package is written in.
  default-language:    Haskell2010


